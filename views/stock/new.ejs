<form action="/stock/create/" method="POST">
  <input type="hidden" name="owner" value="<%= customer.id %>">
  <div class="form-group">
    <label for="exampleInputEmail1">Symbol</label>
    <input type="text" class="form-control" name="symbol">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Number of Shares</label>
    <input type="text" class="form-control" name="number_of_shares">
  </div>
  <div class="form-group">
    <input class="form-control" type="submit" value="Add Stock">
  </div>
</form>
